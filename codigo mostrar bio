    var botao=document.getElementById("btn-bio");
    botao.classList.toggle("artista-active");
    var bio=document.getElementById("bio");
    bio.classList.toggle("bio-active")
    console.log(bio)


    const btn1 = document.querySelector("#btn-bio:nth-child(1)");
    console.log(btn1)
    btn1.onclick=mostrarBio()
  
    function mostrarBio(){
      var botao=document.getElementById("btn-bio");
      botao.classList.toggle("artista-active");
      var bio=document.getElementById("bio");
      bio.classList.toggle("bio-active")
    }  




    for (var i = 0; i < btn_list.length; i++) {
        btn_list[i].onclick=mostrarBio;
      }